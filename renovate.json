{
  "extends": [
    "config:base",
    "docker:enableMajor"
  ],
  "assignees": ["@emmercm"],
  "pinDigests": true,
  "packageRules": [{
    "paths": [
      "0.16/Dockerfile",
      "1.0/Dockerfile"
    ],
    "major": {
      "enabled": false
    },
    "separateMajorMinor": true,
    "minor": {
      "enabled": false
    },
    "separateMinorPatch": true,
    "patch": {
      "enabled": true
    }
  }]
}
